public class Solution {
    public static void main(String[] args)
    {
        int b = 0;
        int sum = 0;
        for(int a=1;a<=10;a++)
        {
            for(b=1;b<=10;b++)
            {
                sum=a*b;
                System.out.print(sum+" ");
            }
            System.out.println();
        }
    }
}
